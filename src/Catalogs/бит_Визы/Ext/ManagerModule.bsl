// Функция - возвращает количество виз в справочнике визы.
// 
// Возвращаемое значение:
//  КолВиз - Число.
//
Функция КоличествоВиз() Экспорт

	Запрос = Новый Запрос;
	Запрос.Текст = "ВЫБРАТЬ
	               |	СУММА(1) КАК Количество
	               |ИЗ
	               |	Справочник.бит_Визы КАК бит_Визы
	               |ГДЕ
	               |	НЕ бит_Визы.ЭтоГруппа";
	
				   
	Результат = Запрос.Выполнить();
	
	Выборка = Результат.Выбрать();
	
	Если Выборка.Следующий() Тогда
	
		КолВиз = ?(ТипЗнч(Выборка.Количество) = Тип("Число"), Выборка.Количество, 0);
		
	Иначе	
		
		КолВиз = 0;
		
	КонецЕсли; 

	Возврат КолВиз;
	
КонецФункции // КоличествоВиз()

