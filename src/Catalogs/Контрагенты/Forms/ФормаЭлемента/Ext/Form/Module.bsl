&НаКлиенте
Процедура ВыполнитьПроверкуРеквизитов(Отказ)
	
	Структура = ВыполнитьПроверкуРеквизитовЗапрос(Объект.Ссылка);
	
	Если Структура <> Неопределено Тогда	
		
		ДанныеОбъекта = Новый Структура("Ссылка, Наименование, ИНН, КПП, ЮридическийАдрес, НаименованиеПолное");
		ЗаполнитьЗначенияСвойств(ДанныеОбъекта, Объект);
		ДанныеОбъекта.ЮридическийАдрес = ?(КонтактнаяИнформацияПолеЮрАдресКонтрагента="Заполнить", "", КонтактнаяИнформацияПолеЮрАдресКонтрагента);
		
		Если Структура.Наименование <> ДанныеОбъекта.Наименование 
			ИЛИ Структура.НаименованиеПолное <> ДанныеОбъекта.НаименованиеПолное 
			ИЛИ Структура.ИНН <> ДанныеОбъекта.ИНН 
			ИЛИ Структура.КПП <> ДанныеОбъекта.КПП 
			ИЛИ Структура.ЮридическийАдрес <> ДанныеОбъекта.ЮридическийАдрес Тогда
			
			ДатаНачалаДействия = ТекущаяДата();
			ВвестиДату(ДатаНачалаДействия, "Укажите дату начала действия новых реквизитов",ЧастиДаты.Дата);
			
			ВыполнитьПроверкуРеквизитовНаСервере(ДатаНачалаДействия, ДанныеОбъекта, Структура, Отказ);
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

&НаСервереБезКонтекста
Функция ВыполнитьПроверкуРеквизитовЗапрос(Ссылка)
	
	 Запрос = Новый Запрос("ВЫБРАТЬ
	                       |	Контрагенты.Ссылка КАК Контрагент,
	                       |	Контрагенты.Наименование,
	                       |	Контрагенты.ИНН,
	                       |	Контрагенты.КПП,
	                       |	ЕСТЬNULL(ВТ_ЮрАдреса.Представление, """") КАК ЮридическийАдрес,
	                       |	Контрагенты.НаименованиеПолное
	                       |ИЗ
	                       |	Справочник.Контрагенты КАК Контрагенты
	                       |		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Контрагенты.КонтактнаяИнформация КАК ВТ_ЮрАдреса
	                       |		ПО (ВТ_ЮрАдреса.Ссылка = Контрагенты.Ссылка)
						   |		И ВТ_ЮрАдреса.Вид = ЗНАЧЕНИЕ(Справочник.ВидыКонтактнойИнформации.ЮрАдресКонтрагента)
	                       |ГДЕ
	                       |	Контрагенты.Ссылка = &Ссылка");
						   
	Запрос.УстановитьПараметр("Ссылка", Ссылка);
	Выборка = Запрос.Выполнить().Выбрать();
	
	Если Выборка.Следующий() Тогда 
		Структура = Новый Структура("Контрагент, Наименование, ИНН, КПП, ЮридическийАдрес, НаименованиеПолное");
		ЗаполнитьЗначенияСвойств(Структура, Выборка);
		Возврат Структура;
	Иначе
		Возврат Неопределено;
	КонецЕсли;
	
КонецФункции

&НаСервереБезКонтекста
Процедура ВыполнитьПроверкуРеквизитовНаСервере(ДатаНачалаДействия, ДанныеОбъекта, Структура, Отказ)
	Если ЗначениеЗаполнено(ДатаНачалаДействия) Тогда
		
		СтруктураОтбора = Новый Структура("Контрагент",ДанныеОбъекта.Ссылка);
		ДанныеРегистра = РегистрыСведений.бит_ИсторияИзмененияРеквизитовКонтрагентов.ПолучитьПоследнее(,СтруктураОтбора);
		Запись = РегистрыСведений.бит_ИсторияИзмененияРеквизитовКонтрагентов.СоздатьМенеджерЗаписи();
		
		ЗаполнитьЗначенияСвойств(Запись,ДанныеРегистра);
		
		Запись.Контрагент = ДанныеОбъекта.Ссылка;
		Запись.ИНН = ДанныеОбъекта.ИНН;
		Запись.КПП = ДанныеОбъекта.КПП;
		Запись.НаименованиеПолное 		= ДанныеОбъекта.НаименованиеПолное;
		Запись.Наименование 			= ДанныеОбъекта.Наименование;
		Запись.ЮридическийАдрес 		= ДанныеОбъекта.ЮридическийАдрес;
		Запись.ДатаМодификации 	= ТекущаяДата();
		Запись.Период			= ТекущаяДата();
		
		//bit auavseenkov  28.01.2015
		Если Структура.Наименование <> ДанныеОбъекта.Наименование Тогда 
			Запись.ДатаНаименование = ДатаНачалаДействия;
		КонецЕсли;
		Если Структура.НаименованиеПолное <> ДанныеОбъекта.НаименованиеПолное Тогда 
			Запись.ДатаНаименованиеПолное = ДатаНачалаДействия;
		КонецЕсли;
		Если Структура.ИНН <> ДанныеОбъекта.ИНН Тогда 
			Запись.ДатаИНН = ДатаНачалаДействия;
		КонецЕсли;
		Если  Структура.КПП <> ДанныеОбъекта.КПП  Тогда 
			Запись.ДатаКПП = ДатаНачалаДействия;
		КонецЕсли;
		Если Структура.ЮридическийАдрес <> ДанныеОбъекта.ЮридическийАдрес Тогда 
			Запись.ДатаЮридическийАдрес =ДатаНачалаДействия;
		КонецЕсли;
		//}
		Запись.Записать();
	Иначе 
		Отказ = Истина;
	КонецЕсли;
КонецПроцедуры

