&НаКлиенте
Процедура ТабличноеПолеОсновныеСредстваПередНачаломИзменения(Элемент, Отказ)
	
	Если Элементы.ОсновныеСредства.ТекущиеДанные = Неопределено Тогда
		Возврат;
	КонецЕсли; 
	Если Элемент.ТекущийЭлемент.Имя = "ТабличноеПолеОсновныеСредстваОбрабатывать" Тогда
		Если НЕ Элемент.ТекущиеДанные.БылоДвижение Тогда
			ПоказатьПредупреждение(,"Значение колонки ""Обрабатывать"" изменить нельзя, т.к. для данной строки не было движений", 6, "Предупреждение");
			Отказ = Истина;
		ИначеЕсли НЕ Элемент.ТекущиеДанные.ДокументКомплектация = ПредопределенноеЗначение("Документ.бит_му_КомплектацияОС.ПустаяСсылка") Тогда
			ПоказатьПредупреждение(,"Значение колонки ""Обрабатывать"" изменить нельзя, т.к. для данной строки заполнено значение документа комплектации", 6, "Предупреждение");
			Отказ = Истина;
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

&НаСервере
Функция ПечатьНажатие_Сервер()
	
	ТабДок = Новый ТабличныйДокумент;
	
	НашМакет =Обработки.бит_му_АнализОттранслированныхПроводокОС.ПолучитьМакет("Макет");
	
	ШапкаДокументаОбласть = НашМакет.ПолучитьОбласть("ШапкаДокумента");
	
	ШапкаДокументаОбласть.Параметры.Организация = Объект.Организация;
	ШапкаДокументаОбласть.Параметры.Период      = Строка(Формат(Объект.ДатаНач, "ДЛФ=Д"))+ " - " + Строка(Формат(Объект.ДатаКон, "ДЛФ=Д"));
	ШапкаДокументаОбласть.Параметры.Настройки   = СокрЛП(Объект.ПостроительОтчета.Настройки.Отбор);
	
	ТабДок.Вывести(ШапкаДокументаОбласть);
	
	ШапкаТаблицыОбласть = НашМакет.ПолучитьОбласть("ШапкаТаблицы");
	ТабДок.ПовторятьПриПечатиСтроки = ТабДок.Вывести(ШапкаТаблицыОбласть);
	
	ТаблицаОбласть = НашМакет.ПолучитьОбласть("Таблица");
	
	Для Каждого ОсновноеСредство ИЗ Объект.ОсновныеСредства Цикл
		ТаблицаОбласть.Параметры.Номер = ОсновноеСредство.НомерСтроки;
		ТаблицаОбласть.Параметры.Обрабатывать = ОсновноеСредство.Обрабатывать;
		ТаблицаОбласть.Параметры.ОС = ОсновноеСредство.ОсновноеСредство;
		ТаблицаОбласть.Параметры.СчетРСБУ = ОсновноеСредство.СчетРСБУ;
		ТаблицаОбласть.Параметры.СубкРСБУ = ОсновноеСредство.СубконтоРСБУ;
		ТаблицаОбласть.Параметры.ДвижРСБУ = ОсновноеСредство.БылоДвижение;
		ТаблицаОбласть.Параметры.Корреспонденция = ОсновноеСредство.ВКорреспонденцииСоСчетомУчетаОС;
		ТаблицаОбласть.Параметры.КорОС = ОсновноеСредство.ОсновноеСредствоКорСчет;
		ТаблицаОбласть.Параметры.Комплектация = ОсновноеСредство.ДокументКомплектация;
		ТаблицаОбласть.Параметры.Проведен = ОсновноеСредство.Проведен;
		ТаблицаОбласть.Параметры.Исключение = ОсновноеСредство.ИсключениеВТрансляции;
		ТаблицаОбласть.Параметры.Комментарий = ОсновноеСредство.Комментарий;
		ТабДок.Вывести(ТаблицаОбласть);
	КонецЦикла;
	
	ПодвалДокументаОбласть = НашМакет.ПолучитьОбласть("ПодвалДокумента");
	ПодвалДокументаОбласть.Параметры.ФИО = Пользователи.ТекущийПользователь().ПолноеНаименование();
	ТабДок.Вывести(ПодвалДокументаОбласть);
	ТабДок.ОриентацияСтраницы = ОриентацияСтраницы.Ландшафт;
	
	//бит_МеханизмыТиповых.НапечататьДокумент(ТабДок, КоличествоЭкземпляров, НаПринтер, "Обработка прпр", НепосредственнаяПечать);
	
	Возврат ТабДок;
КонецФункции


