// Функция возвращает текстовое представление периода отчета с учетом выбранного языка
//
&НаСервере
Функция ПолучитьНадписьПериода()
	
	НазваниеПериода = "";
	
	// рус. квартал
	Если Объект.НачалоПериода = НачалоКвартала(Объект.НачалоПериода) И КонецДня(Объект.КонецПериода) = КонецКвартала(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Русский" Тогда
		Если Месяц(Объект.НачалоПериода) = 1 Тогда
			НомерКвартала = "1";
		ИначеЕсли Месяц(Объект.НачалоПериода) = 4 Тогда
			НомерКвартала = "2";
		ИначеЕсли Месяц(Объект.НачалоПериода) = 7 Тогда
			НомерКвартала = "3";
		Иначе                                                       
			НомерКвартала = "4";
		КонецЕсли;
		НазваниеПериода = НомерКвартала + " квартал " + СтрЗаменить(Строка(Год(Объект.НачалоПериода)), Символы.НПП, "") + " г.";
	// рус. полугодие
	ИначеЕсли (День(Объект.НачалоПериода) = 1 И Месяц(Объект.НачалоПериода) = 1 И День(Объект.КонецПериода) = 30 И Месяц(Объект.КонецПериода) = 6 И Год(Объект.НачалоПериода) = Год(Объект.КонецПериода) Или
		День(Объект.НачалоПериода) = 1 И Месяц(Объект.НачалоПериода) = 7 И День(Объект.КонецПериода) = 31 И Месяц(Объект.КонецПериода) = 12 И Год(Объект.НачалоПериода) = Год(Объект.КонецПериода)) И Объект.ЯзыкОтчета = "Русский" Тогда
		Если Месяц(Объект.НачалоПериода) = 1 Тогда
			НомерПолугодия = "1";
		Иначе
			НомерПолугодия = "2";
		КонецЕсли;
		НазваниеПериода = НомерПолугодия + " полугодие " + СтрЗаменить(Строка(Год(Объект.НачалоПериода)), Символы.НПП, "") + " г.";
	// рус. 9 месяцев
	ИначеЕсли Объект.НачалоПериода = НачалоГода(Объект.НачалоПериода) И День(Объект.КонецПериода) = 30 И Месяц(Объект.КонецПериода) = 9 И Год(Объект.КонецПериода) = Год(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Русский" Тогда
		НазваниеПериода = "9 месяцев " + СтрЗаменить(Строка(Год(Объект.НачалоПериода)), Символы.НПП, "") + " г.";
	// рус. год
	ИначеЕсли Объект.НачалоПериода = НачалоГода(Объект.НачалоПериода) И КонецДня(Объект.КонецПериода) = КонецГода(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Русский" Тогда
		НазваниеПериода = СтрЗаменить(Строка(Год(Объект.НачалоПериода)), Символы.НПП, "") + " г.";
	// англ. квартал
	ИначеЕсли Объект.НачалоПериода = НачалоКвартала(Объект.НачалоПериода) И КонецДня(Объект.КонецПериода) = КонецКвартала(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Английский" Тогда
		НазваниеПериода = "3 months ended " + Формат(Объект.КонецПериода, "Л=en_GB; ДФ=dd.MM.yy");
	// англ. полугодие
	ИначеЕсли (День(Объект.НачалоПериода) = 1 И Месяц(Объект.НачалоПериода) = 1 И День(Объект.КонецПериода) = 30 И Месяц(Объект.КонецПериода) = 6 И Год(Объект.НачалоПериода) = Год(Объект.КонецПериода) Или
		День(Объект.НачалоПериода) = 1 И Месяц(Объект.НачалоПериода) = 7 И День(Объект.КонецПериода) = 31 И Месяц(Объект.КонецПериода) = 12 И Год(Объект.НачалоПериода) = Год(Объект.КонецПериода)) И Объект.ЯзыкОтчета = "Английский" Тогда
		НазваниеПериода = "6 months ended " + Формат(Объект.КонецПериода, "Л=en_GB; ДФ=dd.MM.yy");
	// англ. 9 месяцев
	ИначеЕсли Объект.НачалоПериода = НачалоГода(Объект.НачалоПериода) И День(Объект.КонецПериода) = 30 И Месяц(Объект.КонецПериода) = 9 И Год(Объект.КонецПериода) = Год(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Английский" Тогда
		НазваниеПериода = "9 months ended " + Формат(Объект.КонецПериода, "Л=en_GB; ДФ=dd.MM.yy");
	// англ. год
	ИначеЕсли Объект.НачалоПериода = НачалоГода(Объект.НачалоПериода) И КонецДня(Объект.КонецПериода) = КонецГода(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Английский" Тогда
		НазваниеПериода = "Year " + СтрЗаменить(Строка(Год(Объект.НачалоПериода)), Символы.НПП, "");
	// англ. месяц
	ИначеЕсли Объект.НачалоПериода = НачалоМесяца(Объект.НачалоПериода) И КонецДня(Объект.КонецПериода) = КонецМесяца(Объект.НачалоПериода) И Объект.ЯзыкОтчета = "Английский" Тогда
		НазваниеПериода = ПредставлениеПериода(НачалоДня(Объект.НачалоПериода), КонецДня(Объект.КонецПериода), "Л=en");
	Иначе
		НазваниеПериода = ПредставлениеПериода(НачалоДня(Объект.НачалоПериода), КонецДня(Объект.КонецПериода));
	КонецЕсли;
	
	Возврат НазваниеПериода;
	
КонецФункции // ПолучитьНадписьПериода()

