		Сообщить("Не заполнен периметр консолидации!");
		Отказ = Истина;
	КонецЕсли;
	
	Если Отказ Тогда
		Возврат;
	КонецЕсли;	
	
	Если НЕ ЗначениеЗаполнено(ИмяExcel) Тогда
		Сообщить("Укажите путь к сохраняемому файлу!");	
		Возврат;		
	КонецЕсли;
	
	ЭкспортExcel();

