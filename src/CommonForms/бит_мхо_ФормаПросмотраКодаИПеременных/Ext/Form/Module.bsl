
#Область ОбработчикиСобытийФормы
	
&НаСервере
Процедура ПриСозданииНаСервере(Отказ, СтандартнаяОбработка)
	
	Если Параметры.Свойство("КодОбработчика") Тогда
		ИсполняемыйКод = Параметры.КодОбработчика;
	КонецЕсли;
	
	Если Параметры.Свойство("СтекПеременных") Тогда
		
		ТаблицаПеременные.Очистить();	
		
		Если Параметры.СтекПеременных = Неопределено Тогда
			Возврат;
		КонецЕсли;
		
		инд = -1;
		
		Для Каждого Переменная Из Параметры.СтекПеременных Цикл
			
			НовСтр 				= ТаблицаПеременные.Добавить();
			инд 				= инд + 1;
			НовСтр.Переменная 	= "МассивПеременных[" + Строка(Инд) + "]";
			НовСтр.Значение 	= Переменная;
			
		КонецЦикла;
		
	КонецЕсли;
	
КонецПроцедуры

#КонецОбласти
